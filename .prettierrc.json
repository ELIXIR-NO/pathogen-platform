{
	"semi": true,
	"trailingComma": "es5",
	"singleQuote": false,
	"quoteProps": "as-needed",
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-tailwindcss"]
}
